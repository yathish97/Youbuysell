object scala1 {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
val a="hello"                                     //> a  : String = hello
var b=123                                         //> b  : Int = 123
b=8
println(b)                                        //> 8

val y=10                                          //> y  : Int = 10
val x=20                                          //> x  : Int = 20
val sum=s"sum of $x $y"                           //> sum  : String = sum of 20 10
}